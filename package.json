{
    "name": "devtoberfest2021-frontend-coding-challenge",
    "description": "The sample in this repository has \"built-in\" performance flaws for you to discover and fix during the Frontend Week Coding Challenge as part of Devtoberfest.",
    "version": "0.1.0",
    "author": "SAP SE",
    "license": "Apache-2.0",
    "scripts": {
        "start": "ui5 serve --open index.html",
        "serve": "ui5 serve",
        "build": "ui5 build --clean-dest --dest docs --all ",
        "prettier": "prettier -w .",
        "test": "run-s karma",
        "karma": "run-s clearCoverage karma-ci",
        "clearCoverage": "shx rm -rf coverage",
        "karma-ci": "karma start karma-ci.conf.js"
    },
    "devDependencies": {
        "@ui5/cli": "^2.13.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-ui5": "^2.3.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "shx": "^0.3.3",
        "ui5-middleware-livereload": "^0.5.5"
    },
    "ui5": {
        "dependencies": [
            "ui5-middleware-livereload"
        ]
    }
}
